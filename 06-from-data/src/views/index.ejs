<h1>Daftar Barang EJS FILE</h1>
<table border="1" cellpadding="3" cellspacing="0">
  <tr>
    <th>No</th>
    <th>Kode</th>
    <th>Nama</th>
  </tr>
  <% data.forEach((row, no)=>{ %>
  <tr>
    <td><%= no +1 %></td>
    <td><%= row.id %></td>
    <td><%= row.nama %></td>
  </tr>
  <%}) %>
</table>
<div>
  <h2>Card</h2>
  <div class="card">
    <img src="/img/img_avatar.png" alt="" style="width: 100%" />
    <div class="container">
      <h4><b>Pojok Code</b></h4>
      <p>Lorem ipsum dolor sit amet.</p>
    </div>
  </div>
</div>
<button onclick="show()">Show Message</button>
